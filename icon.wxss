@font-face {
    font-family: 'iconfont';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'), url(data:application/font-woff;charset=utf-8;base64,) format('woff'), url('iconfont.ttf') format('truetype');
    font-weight: normal;
    font-style: normal;
}

.iconfont {
    font-family: "iconfont" !important;
    font-size: 16px;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
}

.icon-like:before {
    content: "\e7c8";
}

.icon-star:before {
    content: "\e7df";
}

.icon-like-fill:before {
    content: "\e869";
}

.icon-star-fill:before {
    content: "\e86a";
}

.icon-comment:before {
    content: "\e8e8";
}